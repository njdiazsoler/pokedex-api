{
  "name": "pokedex-api",
  "version": "1.0.0",
  "description": "api for pokedex web app",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --exec babel-node index.js",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/njdiazsoler/pokedex-api.git"
  },
  "nodemonConfig": {
