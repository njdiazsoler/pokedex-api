  "author": "njdiazsoler",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/njdiazsoler/pokedex-api/issues"
  },
  "homepage": "https://github.com/njdiazsoler/pokedex-api#readme",
  "dependencies": {
    "@babel/node": "^7.10.4",
    "@babel/polyfill": "^7.10.4",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4"
  }
